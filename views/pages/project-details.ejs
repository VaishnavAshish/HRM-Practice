<!DOCTYPE html>
<html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" lang="en">

<% include ../layouts/head %>

<body class="kw">

    <% include ../layouts/nav %>
    <% include ../layouts/sideNav %>

    <main class="main-body-space tpd-60 bpd-100">
        <div>
            <div class="slds-p-around_small">
                <!-- breadcrumb start -->
                <div class="slds-p-bottom_small">
                    <div id="pageRecordNo" style="display:none;"><%= process.env.PAGE_RECORD_NO %></div>
                    <div id="filterCriteria" style="display:none;" ></div>
                    <div id="totalRecordCount" style="display:none;"></div>
                    <div id="offset" style="display:none;">0</div>
                    <nav role="navigation" aria-label="Breadcrumbs" >
                        <ol class="slds-breadcrumb slds-list_horizontal slds-wrap">
                            <li class="slds-breadcrumb__item "><a href="/projects-listing">Projects</a></li>
                            <li class="slds-breadcrumb__item "><a href="javascript:void(0);"><%= project.name %></a></li>
                        </ol>
                    </nav>
                </div>
                <!-- breadcrumb end -->
                <div class="">
                    <div class="slds-tabs_default bg-none">
                        <!-- header start end -->
                        <div class="slds-card">
                            <div class="slds-grid slds-wrap slds-p-horizontal_small slds-p-top_small">
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_3-of-12">
                                    <div class="slds-grid">
                                        <div class="slds-no-flex slds-p-right_x-small">
                                            <span class="slds-icon_container slds-icon-standard-template">
                                                <i class="fa fa-folder-open text-size-24 text-white slds-icon slds-icon_large"></i>
                                            </span>
                                        </div>
                                        <div class="slds-has-flexi-truncate">
                                            <div class="text-size-18 text-height-1">
                                                <strong>
                                                    <%= project.name %>
                                                </strong>
                                            </div>
                                            <div class="max-w-300">
                                                <div class="">
                                                    <div class="slds-grid slds-p-vertical_xx-small text-size-11">
                                                        <div class="slds-has-flexi-truncate">
                                                            <span>
                                                            <!-- <% if(project.status == "Not Started") { %>
                                                                <i class="fa fa-ban text-dull"></i>
                                                            <% } else if(project.status == "In Progress") { %>
                                                                <i class="fa fa-spinner text-orange"></i>
                                                            <% } else if(project.status == "At Risk") { %>
                                                                <i class="fa fa-exclamation-circle text-red"></i>
                                                            <% } else if(project.status == "Completed") { %>
                                                                <i class="fa fa-check-circle text-green"></i>
                                                            <% } %> -->
                                                                <%= project.status %>
                                                            </span>
                                                        </div>
                                                        <div class="slds-no-flex">
                                                                <%= project.percent_completed %>%
                                                        </div>
                                                    </div>
                                                    <div class="slds-progress-bar slds-progress-bar_small slds-progress-bar_circular">
                                                        <span class="slds-progress-bar__value slds-progress-bar__value_success" style="width:<%= project.percent_completed %>%;">
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class=" slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_6-of-12">
                                    <div class="slds-grid slds-wrap slds-p-left_large slds-p-top_x-small">

                                         <div class="slds-size_1-of-1 slds-medium-size_3-of-12">
                                          <div class="slds-form-element slds-m-bottom_medium">
                                              <label class="slds-form-element__label">
                                                  Task Completion
                                              </label>
                                              <div class="slds-form-element__control">
                                                <%= project.total_completed_task_count%>/<%= taskTotalCount %>
                                              </div>
                                          </div>
                                        </div>

                                        <div class="slds-size_1-of-1 slds-medium-size_3-of-12">
                                          <div class="slds-form-element slds-m-bottom_medium">
                                              <label class="slds-form-element__label">
                                                  Total Invoiced Time
                                              </label>
                                              <div class="slds-form-element__control">
                                                <%= project.total_invoice_time %>
                                              </div>
                                          </div>
                                        </div>

                                        <div class="slds-size_1-of-1 slds-medium-size_3-of-12">
                                          <div class="slds-form-element slds-m-bottom_medium">
                                              <label class="slds-form-element__label">
                                                  Total Invoiced Expense
                                              </label>
                                              <div class="slds-form-element__control">
                                                  <% var currency_symbols = currencyWithSymbolArray.filter(function(currency){
                                                      return currency.name == accounts.filter(accountDetail => accountDetail.id == project.account_id)[0].currency;
                                                  })%>
                                                  <span class="text-uppercase text-dull"><%=  currency_symbols[0].symbol %></span>
                                                  <%= project.total_invoice_expense %>
                                              </div>
                                          </div>
                                        </div>

                                        <div class="slds-size_1-of-1 slds-medium-size_3-of-12">
                                            <div class="slds-form-element slds-m-bottom_medium">
                                                <label class="slds-form-element__label">
                                                    Total Invoice Amount
                                                </label>
                                                <div class="slds-form-element__control">
                                                  <span class="text-uppercase text-dull"><%=  currency_symbols[0].symbol %></span>
                                                  <%= project.total_invoice_amount %>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>

                                <div class="slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_3-of-12">
                                    <div class="text-right sm-tpd-15">

                                       <!-- <button class="slds-button slds-button_neutral slds-align-top" onclick="exportToCsv('generateProjectDetailCsv/<%= project.id%>')">
                                            <i class="fa fa-file-export"></i>
                                         <span class="sm-hide">Export Project Details</span>
                                       </button> -->
                                       <% if(user.permissions.includes('projectManager')) { %>
                                         <button class="slds-button slds-button_icon slds-button_icon-border-filled slds-align-top" onclick="deleteBeforeConfirm('#globalAlert', 'deleteProject(\'<%= project.id %>\')','Are you sure you want to delete this project?');" title="Delete">
                                                  <!-- updateProjectDetails('<%= project.id %>', '#edit-project') -->
                                                <span class="slds-button__icon">
                                                  <i class=" fa fa-trash text-red" aria-hidden="true"></i>
                                                </span>
                                          </button>
                                        <% } %>

                                    </div>
                                </div>
                            </div>
                            <div class="slds-p-top_x-small">
                                <ul class="slds-tabs_default__nav radius-4 scroll-x-auto bpd-2 text-size-14">
                                    <% if(user.permissions.includes('projectManager')) { %>
                                      <li data-navigation="settings" class="slds-tabs_default__item slds-is-active" onclick="openTab(this, 'project-settings')">
                                          <a class="slds-tabs_default__link" href="javascript:void(0);" tabindex="-1">
                                              Settings
                                          </a>
                                      </li>
                                    <% } %>
                                    <li data-navigation="tasks" class="slds-tabs_default__item" onclick="openTab(this, 'project-tasks')">
                                        <a class="slds-tabs_default__link" href="javascript:void(0);" tabindex="0">
                                            Tasks (<%= taskTotalCount %>)
                                        </a>
                                    </li>

                                    <li data-navigation="activity" class="slds-tabs_default__item" onclick="openTab(this, 'task-forums')">
                                        <a class="slds-tabs_default__link" href="javascript:void(0);" tabindex="0" id="conversationCount">
                                            Activity (<%= projectConversationThread.length %>)
                                        </a>
                                    </li>
                                    <li data-navigation="timeline" class="slds-tabs_default__item" onclick="openTab(this, 'project-timeline');">
                                        <a class="slds-tabs_default__link" href="javascript:void(0);" tabindex="0" id="projectGanttChart">
                                            Timeline
                                        </a>
                                    </li>
                                    <!-- <li class="slds-tabs_default__item" onclick="openTab(this, 'project-files')">
                                        <a class="slds-tabs_default__link" href="javascript:void(0);" tabindex="-1">
                                            Files (3)
                                        </a>
                                    </li>
                                    <li class="slds-tabs_default__item" onclick="openTab(this, 'project-analytics')">
                                        <a class="slds-tabs_default__link" href="javascript:void(0);" tabindex="-1">
                                            Analytics
                                        </a>
                                    </li> -->
                                </ul>
                            </div>

                        </div>
                        <!-- header end -->
                        <div>

                            <!-- tasks tab start -->
                            <div class="slds-tabs_default__content hide" target-tab="project-tasks">
                                <div class=" slds-card ">
                                    <!-- task header start -->
                                    <div class="slds-p-around_small">
                                        <div class="slds-grid slds-wrap slds-grid_pull-padded">
                                            <div class="slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_5-of-12 slds-p-horizontal_small">
                                                <div class="slds-grid">

                                                    <!-- <div class="slds-p-right_medium sm-hide">
                                                        <div class="slds-grid">
                                                            <div class="slds-no-flex">
                                                                <label class="slds-form-element__label slds-p-top_x-small" for="">
                                                                    Sort :
                                                                </label>
                                                            </div>
                                                            <div class="">
                                                                <div class="slds-select_container">
                                                                    <select class="slds-select">
                                                                        <option value="">Recent</option>
                                                                        <option>Other</option>
                                                                        <option>Other</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div> -->
                                                   <!--  <div class="slds-no-flex slds-p-right_small">
                                                        <button class="slds-button slds-button_icon slds-button_icon-border-filled slds-align-top" title="filter">
                                                            <span class="slds-button__icon">
                                                                <i class=" fa fa-filter" aria-hidden="true"></i>
                                                            </span>
                                                        </button>
                                                    </div> -->
                                                    <div class="sm-slds-grow">
                                                        <div class="slds-form-element">
                                                            <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                                                <input type="text" class="slds-input" placeholder="Search task by name" id="searchTaskName" searchFunc="findTaskByName()" onkeypress="handle(event,this);">
                                                                <div class="slds-input__icon-group slds-input__icon-group_right">
                                                                    <button class="slds-input__icon slds-input__icon_right slds-button slds-button_icon" title="Clear" onclick="clearSearchBox(this);findTaskByName();">
                                                                    <i class="fa fa-times-circle"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="slds-no-flex slds-p-left_small">
                                                        <button class="slds-button slds-button_icon slds-button_icon-border-filled slds-align-top" title="Search" onclick="findTaskByName();">
                                                        <span class="slds-button__icon">
                                                            <i class=" fa fa-search" aria-hidden="true"></i>
                                                        </span>
                                                        </button>
                                                    </div>
                                                    <!-- <div class="sm-slds-grow">
                                                        <div class="slds-form-element">
                                                            <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_left-right slds-input-has-icon_group-right">
                                                                <span class=" slds-input__icon_left slds-input__icon">
                                                                    <i class="fa fa-search"></i>
                                                                </span>
                                                                <input type="text" class="slds-input" placeholder="Search">
                                                                <div class="slds-input__icon-group slds-input__icon-group_right">
                                                                    <button class="slds-input__icon slds-input__icon_right slds-button slds-button_icon" title="Clear">
                                                                        <i class="fa fa-times-circle"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div> -->

                                                </div>
                                            </div>
                                            <div class="slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_7-of-12 slds-p-horizontal_small">
                                                <div class="text-right sm-tpd-15">
                                                  <!-- <% if(taskTotalCount>0){ %>
                                                    <button class="slds-button slds-button_neutral slds-align-top" onclick="exportToCsv('generateTaskCsv/<%= project.id %>')">
                                                        <span class="">Export Task</span>
                                                    </button>
                                                  <% } else { %>
                                                    <button class="slds-button slds-button_neutral slds-align-top" onclick="exportToCsv('generateTaskCsv/<%= project.id %>')" disabled>
                                                        <span class="">Export Task</span>
                                                    </button>
                                                  <% } %> -->
                                                    <button class="slds-button slds-button_brand slds-align-top" onclick="openModal('#modalAddNewTask', true)">
                                                        <i class="fa fa-plus"></i>
                                                        <span class="">Add Task</span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- task header end -->
                                    <div class="slds-p-horizontal_small slds-p-bottom_small" id="taskCardView">
                                      <ul id="taskSortWrapper" class="kw-task-sort-wrapper">
                                        <% if(tasks.length > 0) { %>
                                            <% tasks.forEach(function(task) {%>
                                                <!-- row start -->
                                                <li id="<%= task.id %>" >
                                                    <div class=" slds-m-bottom_medium slds-p-around_small line-around kw-task-item">
                                                        <div class="slds-grid slds-wrap slds-grid_pull-padded">
                                                            <div class="slds-size_1-of-1 slds-medium-size_1-of-1  slds-large-size_5-of-12 slds-p-horizontal_small">
                                                                <div class="text-size-11 text-dull text-uppercase slds-p-bottom_xx-small">
                                                                    <span class="kw-drag-handle slds-p-around_small" title="Drag & Drop to Reorder"> </span>
                                                                    <%= task.record_id %>
                                                                    <span class="slds-badge slds-m-left_small">
                                                                        <% if(task.billable) { %>
                                                                            Billable
                                                                        <% } else { %>
                                                                            Non Billable
                                                                        <% } %>
                                                                    </span>
                                                                </div>
                                                                <div class="sm-bpd-15">
                                                                    <a href="/task-details?taskId=<%= task.id %>#details">
                                                                        <%= task.name %>
                                                                    </a>
                                                                </div>
                                                            </div>

                                                            <div class="slds-size_1-of-1 slds-medium-size_1-of-1  slds-large-size_7-of-12 slds-p-horizontal_small">
                                                                <div class="slds-grid slds-wrap slds-grid_pull-padded">

                                                                    <div class="slds-size_1-of-1 slds-medium-size_3-of-12 slds-p-horizontal_small sm-tpd-15">
                                                                        <div class="slds-truncate text-dull slds-p-bottom_xx-small text-size-11 text-uppercase">
                                                                            Status
                                                                        </div>
                                                                        <div>
                                                                            <!-- <% if(task.status == 'Not Started') {%>
                                                                                <i class="fa fa-ban text-dull"></i>
                                                                            <% } else if(task.status == 'In Progress') {%>
                                                                                <i class="fa fa-spinner text-orange"></i>
                                                                            <% } else if(task.status == 'At Risk') {%>
                                                                                <i class="fa fa-exclamation-circle text-red"></i>
                                                                            <% } else if(task.status == 'Completed') {%>
                                                                                <i class="fa fa-check-circle text-green"></i>
                                                                            <% } %> -->
                                                                            <%= task.status %>
                                                                        </div>
                                                                    </div>

                                                                    <div class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_small">
                                                                        <div class="slds-grid slds-grid_pull-padded">
                                                                            <div class="slds-size_1-of-2 slds-p-horizontal_small">
                                                                                <div class="slds-truncate text-dull slds-p-bottom_xx-small text-size-11 text-uppercase">
                                                                                    Start Date
                                                                                </div>
                                                                                <div>

                                                                                    <%= task.startDateFormatted %>
                                                                                </div>
                                                                            </div>
                                                                            <div class="slds-size_1-of-2 slds-p-horizontal_small">
                                                                                <div class="slds-truncate text-dull slds-p-bottom_xx-small text-size-11 text-uppercase">
                                                                                    Due Date
                                                                                </div>
                                                                                <div>
                                                                                    <%= task.endDateFormatted %>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>



                                                                    <div class="slds-size_1-of-1 slds-medium-size_3-of-12 slds-p-horizontal_small sm-tpd-15">
                                                                        <div class="slds-grid">
                                                                             <div class="slds-has-flexi-truncate">
                                                                                <div class="slds-truncate text-dull slds-p-bottom_xx-small text-size-11 text-uppercase">
                                                                                                    Assigned
                                                                                </div>
                                                                                <div>
                                                                                  <% if(task.user_id) { %>
                                                                                    <span class="slds-avatar slds-avatar_small slds-avatar_circle cursor-pointer">
                                                                                        <img alt="Person name" src="/getUserPicture/<%= task.user_id %>" title="<%= task.user_first_name %> <%= task.user_last_name?task.user_last_name:'' %> (<%= task.user_role %>)">
                                                                                     </span>
                                                                                   <% } %>
                                                                                </div>
                                                                            </div>
                                                                            <div class="slds-col--bump-left slds-p-left_x-small">
                                                                                <a href="/task-details?taskId=<%= task.id %>" class="slds-button slds-button_icon" title="Edit">
                                                                                    <i class="fa fa-edit text-blue fs-lg text-size-18"></i>
                                                                                </a>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </li>
                                                <!-- row end -->
                                             <% }); %>
                                        <% } %>
                                      </ul>
                                    </div>
                                    <div class="slds-text-align_center slds-p-vertical_medium">
                                        <button class="slds-button" loadItemId="loadMoreBtn" onclick="loadMoreTask()">&nbsp;</button>
                                    </div>
                                    <% if(tasks.length <= 0) { %>
                                        <div class="slds-p-around_small text-center" title="" id="noTaskFound" >
                                            No task found.
                                        </div>
                                    <% }  else { %>
                                        <div class="slds-p-around_small text-center slds-hide" title="" id="noTaskFound" >
                                            No task found.
                                        </div>
                                    <% } %>
                                </div>
                            </div>
                            <!-- tasks tab end -->

                            <!-- settings tab start -->
                            <% if(user.permissions.includes('projectManager')) { %>
                              <div class="slds-tabs_default__content" target-tab="project-settings">
                                <div class=" slds-card slds-p-around_large sm-pd-10" data-modified id="editProjectForm">
                                    <div class="slds-grid slds-wrap slds-grid_pull-padded">
                                        <div class="slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_6-of-12 slds-p-horizontal_small">
                                            <div class="slds-p-bottom_x-small line-bottom">
                                                <div class="text-uppercase ">
                                                    Settings
                                                </div>
                                            </div>
                                            <form id="edit-project">
                                                <div class="slds-p-top_large">
                                                    <div>
                                                        <div class="slds-form-element slds-m-bottom_medium">
                                                            <label class="slds-form-element__label">
                                                                Project Title
                                                                <abbr class="slds-required" title="required">*</abbr>
                                                            </label>
                                                            <div class="slds-form-element__control">
                                                                <input type="text" id="project_title" name="project_title" class="slds-input" getFormData='' placeholder="" value="<%= project.name %>" required="">
                                                                <div class="slds-text-color_error text-size-12 slds-p-top_xx-small">

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="slds-form-element slds-m-bottom_medium">
                                                            <label class="slds-form-element__label" for="">Note</label>
                                                            <div class="slds-form-element__control">
                                                                <textarea id="" class="slds-textarea" getFormData='' name="project_desc" placeholder=""><%= project.description %></textarea>
                                                                <div class="slds-text-color_error text-size-12 slds-p-top_xx-small">

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="slds-form-element slds-m-bottom_medium">
                                                            <label class="slds-form-element__label">
                                                                Clients/Accounts
                                                            </label>
                                                            <div class="slds-form-element__control">
                                                                <div class="slds-select_container">
                                                                    <select class="slds-select" getFormData='' name="account" id="account">
                                                                        <% if(accounts.length > 0) { %>
                                                                            <option value=""> </option>
                                                                            <% accounts.forEach(function(account) { %>
                                                                                <option value="<%= account.id %>"><%= account.name %></option>
                                                                            <% }); %>
                                                                        <% } else { %>
                                                                            <option value=""> </option>
                                                                        <% } %>
                                                                    </select>
                                                                </div>
                                                                <div class="slds-text-color_error text-size-12 slds-p-top_xx-small">

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="slds-grid slds-wrap slds-gutters">
                                                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12">
                                                                <div class="slds-form-element slds-m-bottom_medium">
                                                                    <label class="slds-form-element__label">
                                                                        Start Date
                                                                    </label>
                                                                    <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                                                        <span class="slds-input__icon slds-input__icon_right">
                                                                            <i class="fa fa-calendar"></i>
                                                                        </span>
                                                                        <input type="date" id="pr_start_date"  class="slds-input open-date-pik" getFormData='' name="start_date" value="<%= project.startDateFormatted %>" />
                                                                    </div>
                                                                    <div class="slds-text-color_error text-size-12 slds-p-top_xx-small">

                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12">
                                                                <div class="slds-form-element slds-m-bottom_medium">
                                                                    <label class="slds-form-element__label">
                                                                        End Date
                                                                    </label>
                                                                    <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                                                        <span class="slds-input__icon slds-input__icon_right">
                                                                            <i class="fa fa-calendar"></i>
