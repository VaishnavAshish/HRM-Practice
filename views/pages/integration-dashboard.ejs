                                                                <div class="slds-has-flexi-truncate slds-p-horizontal_small">
                                                                     <div class="slds-truncate">
                                                                         All
                                                                     </div>
                                                                </div>
                                                            </a>
                                                        </li>
                                                        <!-- <li class="slds-dropdown__item">
                                                            <a href="javascript:void(0);" tabindex="0" class="slds-grid_vertical-align-center">
                                                                <div class="slds-has-flexi-truncate slds-p-horizontal_small">
                                                                     <div class="slds-truncate">
                                                                         Active
                                                                     </div>
                                                                </div>
                                                                <div class="slds-no-flex">
                                                                    <span class="slds-badge slds-align-middle"> 1 </span>
                                                                </div>
                                                            </a>
                                                        </li> -->

                                                    </ul>
                                                </div>
                                            </div>
                                        </h1>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size_1-of-1 slds-medium-size_1-of-2">
                                <div class="slds-text-align_right sm-tpd-10 sm-text-left">

                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Integrations header end -->

                    <!-- list start -->
                    <div class="slds-card slds-p-around_small" style="min-height: 70vh">
                        <ul class="slds-grid slds-grid_pull-padded slds-wrap">
                            <li class="slds-p-horizontal_small slds-size_1-of-1 slds-medium-size_1-of-3">
                              <% if(!user.company_info.xero_enabled){ %>
                                <a href="javascript:void(0);" class="slds-app-launcher__tile slds-text-link_reset slds-m-vertical_medium " onclick="openModal('#modalQuickbookSetting', true)">
                              <% } else { %>
                                <a href="javascript:void(0);" class="slds-app-launcher__tile slds-text-link_reset slds-m-vertical_medium ">
                              <% } %>
                                    <div class="slds-no-flex slds-p-around_small">
