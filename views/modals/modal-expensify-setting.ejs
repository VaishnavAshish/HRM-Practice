<div id="modalExpensifySetting">
    <section role="dialog" tabindex="-1" class="slds-modal">
      <!-- modal expensify setting start-->
              <div class="slds-modal__container">
                  <header class="slds-modal__header">
                      <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close" onclick="closeModal('#modalExpensifySetting');">
                          <span class="slds-button__icon">
                              <i class="fa fa-times text-size-21"></i>
                          </span>
                      </button>
                      <h2  class="slds-text-heading_medium slds-hyphenate">Integration With Expensify</h2>
                  </header>

                      <!-- <% if(!user.company_info.quickbook_enabled){ %> -->
                        <form id="connect-to-expensify" method="get" action="/initiateExpensify">
                            <div class="slds-modal__content slds-p-around_medium">
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_12-of-12">
                                    <div class="slds-form-element slds-m-bottom_medium">
                                        <label class="slds-form-element__label" for="partner_user_id"> Partner user id </label>
                                        <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                            <input type="text" id="partner_user_id" name="partner_user_id" class="slds-input" required>
                                        </div>
                                    </div>
                                    <div class="slds-form-element slds-m-bottom_medium">
                                        <label class="slds-form-element__label" for="partner_user_secret"> Partner user secret </label>
                                        <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                            <input type="text" id="partner_user_secret" name="partner_user_secret" class="slds-input" required>
                                        </div>
                                    </div>                                    
                                </div>
                            </div>
                        
                            <footer class="slds-modal__footer">
                                <button class="slds-button slds-button_neutral" onclick="closeModal('#modalExpensifySetting');">Cancel</button>
                                <button class="slds-button" type="submit">
                                    <img src="/img/expensify_thumbnail.svg" alt="" width="25">
                                </button>
                            </footer>
                        </form>
                    <!-- <% } else { %> -->
                        <!-- <div class="slds-modal__content slds-p-around_medium">
                            <p> The connection with expensify is done at : <%= (last_integration_time) ? last_integration_time : '' %> </p>
                        </div>
                        <footer class="slds-modal__footer">
                          <button class="slds-button slds-button_neutral" onclick="closeModal('#modalExpensifySetting');">Cancel</button>
                          <button class="slds-button slds-button_brand" onclick="disconnectIntegration('#modalExpensifySetting','Expensify');">Disconnect To Expensify</button>
                        </footer/> -->
                    <!-- <% } %> -->
              </div>
          </section>
          <div class="slds-backdrop"></div>
      </div>
      <!-- modal quickbook setting end-->
