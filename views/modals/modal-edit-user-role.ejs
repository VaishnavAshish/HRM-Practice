<!-- modal edit user role start-->        
<div id="modalEditUserRole">
    <section role="dialog" tabindex="-1" class="slds-modal">
        <div class="slds-modal__container">
            <header class="slds-modal__header">
                <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close" onclick="closeModal('#modalEditUserRole');">
                    <span class="slds-button__icon">
                        <i class="fa fa-times text-size-21"></i>
                    </span>
                </button>
                <h2  class="slds-text-heading_medium slds-hyphenate">Edit User Role</h2>
            </header>
            <form id="editUserRole">
            <div class="slds-modal__content slds-p-around_medium">
                <div>
                    <div class="slds-form-element slds-m-bottom_medium">
                        <label class="slds-form-element__label">
                            Role Name
                            <abbr class="slds-required" title="required">*</abbr>
                        </label>
                        <div class="slds-form-element__control">
                            <input type="text" class="slds-input" placeholder="" required="" id="roleNameEdit" name="roleNameEdit" getFormData/>
                            <div class="slds-text-color_error text-size-12 slds-p-top_xx-small">
                                       
                            </div>
                        </div>
                        <div class="slds-form-element__control">
                            <input type="text" class="slds-input" placeholder="" required="" style="display:none;" id="userRolePrev" name="userRolePrev" getFormData/>
                            <div class="slds-text-color_error text-size-12 slds-p-top_xx-small">
                                       
                            </div>
                        </div>
                    </div>
                    <!-- <div class="slds-form-element slds-m-bottom_medium">
                        <div class="slds-form-element__control">
                            <span class="slds-checkbox">
                                <input type="checkbox"  id="Eble" value="" checked="" id="enableRoleEdit" name="enableRoleEdit" getFormData>
                                <label class="slds-checkbox__label" for="Eble">
                                    <span class="slds-checkbox_faux"></span>
                                    <span class="slds-form-element__label">Enable</span>
                                </label>
                            </span>
                        </div>
                    </div> -->
                    
                            
                </div>
            </div>
        </form>
            <footer class="slds-modal__footer">
                <div class="slds-grid">
                    <div class="slds-has-flexi-truncate">
                        <div class="text-left">
                            <button class="slds-button slds-button_destructive" onclick="deleteUserRoleEdit('#modalEditUserRole')">Delete</button>    
                        </div>
                    </div>
                    <div class="slds-no-flex">
                        <button class="slds-button slds-button_neutral" onclick="closeModal('#modalEditUserRole');">Cancel</button>
                        <button class="slds-button slds-button_brand" onclick="editNewUserRole('#modalEditUserRole')">Edit</button>
                    </div>
                </div>
            </footer>
        </div>
    </section>
    <div class="slds-backdrop"></div>
</div>
<!-- modal edit user role end-->   